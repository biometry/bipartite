  issn =          {0027-8424},
  keywords =      {Agricultural,Agricultural: economics,Animals,Bees,Bees: physiology,Chimera,Chimera: physiology,Conservation of Natural Resources,Crops,Helianthus,Helianthus: growth {\&} development,Helianthus: physiology,Honey,Pollen,ZZmuell,bipartite},
  mendeley-tags = {ZZmuell,bipartite},
  pmid =          {16940358},
