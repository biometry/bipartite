@Article{Sorensen2011,
  Title                    = {A method for under-sampled ecological network data analysis: plant-pollination as case study},
  Author                   = {S{\o}rensen, Peter B and Damgaard, Christian F and Strandberg, Beate and Dupont, Yoko L and Marianne, B and Carvalheiro, Lu{\'{\i}}sa Gigante and Biesmeijer, Jacobus C and Olsen, Jens Mogens and Hagen, Melanie and Potts, G},
