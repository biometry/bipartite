@Article{AlmeidaNeto2007,
  Title                    = {On nestedness analyses: rethinking matrix temperature and anti-nestedness},
  Author                   = {Almeida-Neto, M{\'{a}}rio and Lewinsohn, Thomas Michael and {Guimar{\~{a}}es Jr.}, Paulo R.},
  Journal                  = {Oikos},
  Year                     = {2007},
  Pages                    = {716--722},
  Volume                   = {116},

  Abstract                 = {The analysis of nested structures in sets of species assemblages across different sites or in networks of interspecific interactions has become common practice in ecological studies. Although new analyses and metrics have been proposed, few studies have scrutinized the concepts that subtend nestedness analysis. We note two important conceptual problems that can lead to terminological inconsistencies and flawed interpretations. First, the thermodynamic analogy that underlies the most common metric of nestedness, matrix temperature, is flawed and has led some authors to incorrect interpretations. Second, the term ‚Äò‚Äòanti-nestedness‚Äô‚Äô is a potential source of confusion and inconsistencies. We review four concepts for anti-nestedness and examine how distinct they are. ‚Äò‚ÄòAnti-nested‚Äô‚Äô matrices, i.e. less nested than expected by chance, may result from different ecological processes and show distinct structural patterns. Thus, there is no single unequivocal opposite of nestedness to be represented as ‚Äò‚Äòanti-nestedness‚Äô‚Äô. A more profitable approach is to designate and test each distinct non-nested pattern according to its specific assumptions and mechanistic hypotheses.},
  Doi                      = {10.1111/j.2007.0030-1299.15803.x},
  Keywords                 = {bipartite},
  Mendeley-tags            = {bipartite},
  Timestamp                = {2015.12.02}
}

