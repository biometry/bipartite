@Article{Vazquez2002,
  Title                    = {Ecological specialization and susceptibility to disturbance: conjectures and refutations},
  Author                   = {V\'{a}zquez, Diego P and Simberloff, Daniel},
  Journal                  = {The American Naturalist},
  Year                     = {2002},

  Month                    = jul,
  Number                   = {6},
