@Article{Vazquez2009,
  Title                    = {Uniting pattern and process in plant-animal mutualistic networks: a review},
  Author                   = {V\'{a}zquez, Diego P and Bl\"{u}thgen, Nico and Cagnolo, Luciano and Chacoff, Natacha P},
