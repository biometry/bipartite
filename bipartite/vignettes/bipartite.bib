@Article{Chamberlain2014,
  author       = {Chamberlain, Scott and Vázquez, Diego P. and Carvalheiro, Luisa and Elle, Elizabeth and Vamosi, Jana C.},
  title        = {Phylogenetic tree shape and the structure of mutualistic networks},
