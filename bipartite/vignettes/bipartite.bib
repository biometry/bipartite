@Article{Rossberg2011,
  Title                    = {Universal power-law diet partitioning by marine fish and squid with surprising stability-diversity implications},
  Author                   = {Rossberg, Axel G and Farnsworth, Keith D and Satoh, Keisuke and Pinnegar, John K},
  Journal                  = {Proceedings of the Royal Society of London B},
